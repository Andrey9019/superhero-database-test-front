{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "jsx": "react-jsx",
    "baseUrl": "src",
    "types": ["vite/client", "node" ],
    "outDir": "dist",
     "allowImportingTsExtensions": true,
      "noEmit": true

  },
  "include": ["src"],
  "exclude": ["node_modules", "dist"]
}
